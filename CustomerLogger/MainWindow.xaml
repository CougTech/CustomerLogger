<Window x:Class="CustomerLogger.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow"
        WindowStyle="None"
        WindowState="Maximized">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="#FFE2E2E2"/>
                <GradientStop Color="White" Offset="0.064"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Frame x:Name="ContentFrame" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#efefef" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" NavigationUIVisibility="Visible" IsTabStop="False">
        </Frame>
        <Image x:Name="cougtechImage" Margin="0,10,10,0" Source="csd.png" Height="200" VerticalAlignment="Top" HorizontalAlignment="Right" Width="325"/>

    </Grid>
</Window>
