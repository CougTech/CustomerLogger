<Window x:Class="CustomerLogger.PasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CustomerLogger"
        mc:Ignorable="d"
        Title="Admin" Height="130.07" Width="349.301" ResizeMode="NoResize" ShowInTaskbar="False">
    <Grid>

        
        <Button x:Name="okButton" Content="OK" HorizontalAlignment="Left" Margin="126,68,0,0" VerticalAlignment="Top" Width="75" Click="okButton_Click"/>
        <PasswordBox x:Name="passwordBox" PasswordChar="*" MaxLength="32" HorizontalAlignment="Left" Margin="10,24,0,0" VerticalAlignment="Top" Width="313" Height="24" KeyUp="passwordBox_KeyUp"/>
        <Label x:Name="pwLabel" Content="Enter Password:" HorizontalAlignment="Left" Margin="5,0,0,0" VerticalAlignment="Top" Width="328"/>

    </Grid>
</Window>
